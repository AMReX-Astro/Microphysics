&PROBIN
  test_set = "gr0_3d.128"

  dens_min   = 1.d-3
  dens_max   = 5.d9
  temp_min   = 1.d7
  temp_max   = 6.d8

  tmax = 1.0d-1

  xin_file   = "xin.reaclib_cburn.128"
  run_prefix = "react_reaclib_cburn_128_"

  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = .false.

  call_eos_in_rhs = T

  small_x = 1.d-16

  rtol_spec = 1.0d-12
  rtol_enuc = 1.0d-9
  atol_spec = 1.0d-12
  atol_enuc = 1.0d-9
  
/
