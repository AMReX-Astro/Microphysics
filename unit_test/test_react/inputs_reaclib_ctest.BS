&PROBIN
  test_set = "gr0_3d.small"

  dens_min   = 1.d9
  dens_max   = 1.d9
  temp_min   = 1.d9
  temp_max   = 1.d9

  tmax = 1.0d-6

  xin_file   = "xin.reaclib_ctest"
  run_prefix = "react_reaclib_ctest_"

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  burner_verbose = .true.

  rtol_spec = 1.d-3
  rtol_temp = 1.d-3
  rtol_enuc = 1.d-3
  atol_spec = 1.d-3
  atol_temp = 1.d-3
  atol_enuc = 1.d-3
  renormalize_abundances = .true.
  
/
