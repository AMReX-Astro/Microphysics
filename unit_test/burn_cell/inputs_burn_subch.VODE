&PROBIN

  run_prefix = "react_subch_"
  
  small_temp = 1d5
  small_dens = 1d5
  
  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = .false.

  rtol_spec = 1.0d-9
  rtol_enuc = 1.0d-6
  rtol_temp = 1.0d-6
  atol_spec = 1.0d-9
  atol_enuc = 1.0d-6
  atol_temp = 1.0d-6

/

&cellparams

  tmax     = 1.0d3
  numsteps = 1000

  density       = 1.0d6
  temperature   = 0.3d9

  massfractions(1)  = 0.0000000000000000d0
  massfractions(2)  = 0.0000000000000000d0
  massfractions(3)  = 0.2075000000000000d0
  massfractions(4)  = 0.0066666666666667d0
  massfractions(5)  = 0.0053571430000000d0
  massfractions(6)  = 0.0000000000000000d0
  massfractions(7)  = 0.0000000000000000d0
  massfractions(8)  = 0.0000000000000000d0
  massfractions(9)  = 0.0000000000000000d0

/
