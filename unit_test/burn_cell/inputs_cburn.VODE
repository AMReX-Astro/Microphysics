&PROBIN

  run_prefix = "reaclib_cburn_"

  small_temp = 1d5
  small_dens = 1d5

  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = .true.

  rtol_spec = 1.0d-12
  rtol_enuc = 1.0d-9
  rtol_temp = 1.0d-9
  atol_spec = 1.0d-12
  atol_enuc = 1.0d-9
  atol_temp = 1.0d-9

  small_x = 1.d-16
  
/
