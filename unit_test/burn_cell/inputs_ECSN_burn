&PROBIN

  run_prefix = "react_ECSN_"
  
  small_temp = 1d5
  small_dens = 1d5
  
  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = T

  rtol_spec = 1.0d-9
  rtol_enuc = 1.0d-6
  rtol_temp = 1.0d-6
  atol_spec = 1.0d-9
  atol_enuc = 1.0d-6
  atol_temp = 1.0d-6

/

&cellparams

  tmax     = 1.0d-4
  numsteps = 5

  density       = 8.01d7
  temperature   = 1.84d8

  massfractions(1)  = 1.0d-10
  massfractions(2)  = 1.0d-10
  massfractions(3)  = 0.6d0
  massfractions(4)  = 1.0d-10
  massfractions(5)  = 1.0d-10
  massfractions(6)  = 0.4d0
  massfractions(7)  = 1.0d-10
  massfractions(8)  = 1.0d-10
  massfractions(9)  = 1.0d-10
  massfractions(10) = 1.0d-10
  massfractions(11) = 1.0d-10
/
