&PROBIN

  run_prefix = "test_ECSN_"
  
  small_temp = 1d5
  small_dens = 1d5
  
  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = T

  rtol_spec = 1.0d-9
  rtol_enuc = 1.0d-6
  rtol_temp = 1.0d-6
  atol_spec = 1.0d-9
  atol_enuc = 1.0d-6
  atol_temp = 1.0d-6

/

&cellparams

  tmax     = 1.0d-4
  numsteps = 1

  density       = 8.3d9
  temperature   = 7.d8

  massfractions(1)  = 0.5d0
  massfractions(2)  = 0.5d0
/
