&PROBIN

  run_prefix = "react_aprox13_"
  
  small_temp = 1d5
  small_dens = 1d5
  
  burner_verbose = .false.

  ! Set which jacobian to use
  ! 1 = analytic jacobian
  ! 2 = numerical jacobian
  jacobian   = 1

  renormalize_abundances = T

  rtol_spec = 1.0d-9
  rtol_enuc = 1.0d-6
  rtol_temp = 1.0d-6
  atol_spec = 1.0d-9
  atol_enuc = 1.0d-6
  atol_temp = 1.0d-6

/

&cellparams

  tmax     = 1.0d-4
  numsteps = 100

  density       = 1.d7
  temperature   = 1.d9

  massfractions(1)  = 1.0d0
  massfractions(2)  = 0.0d0
  massfractions(3)  = 0.0d0
  massfractions(4)  = 0.0d0
  massfractions(5)  = 0.0d0
  massfractions(6)  = 0.0d0
  massfractions(7)  = 0.0d0
  massfractions(8)  = 0.0d0
  massfractions(9)  = 0.0d0
  massfractions(10) = 0.0d0
  massfractions(11) = 0.0d0
  massfractions(12) = 0.0d0
  massfractions(13) = 0.0d0

/
